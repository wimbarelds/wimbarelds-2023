{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    // "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "types": ["solid-start/env"],
    "baseUrl": "./",
    
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
